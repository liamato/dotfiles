[alias]
	## Spellings
	ad = add
	ass = add
	dff = diff
	doff = diff
	emrge = merge
	idff = diff
	mergfe = merge
	mregetool = mergetool
	oull = pull
	pish = push
	pl = pull
	ps = push
	psuh = push
	pu = up
	puhs = push
	puosh = push
	pus = push
	pushy = push
	reabse = rebase
	satsh = stash

	## Shortcuts
	git = "!exec git"
	alias = "!sh -c '[ $# = 2 ] && git config --global alias.\"$1\" \"$2\" && exit 0 || echo \"usage: git alias <new alias> <original command>\" >&2 && exit 1' -"
	l = log
	lg = "log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all"
	a = add
	c = commit
	ck = checkout
	d = diff
	s = status
	b = branch

[color]
	branch = auto
	diff = auto
	status = auto
	ui = true

